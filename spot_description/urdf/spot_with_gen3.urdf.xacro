    <xacro:arg name="dof" default="6" />
    <xacro:arg name="vision" default="true" />

    <xacro:arg name="gripper" default="robotiq_2f_140" />

    <xacro:arg name="sim" default="false" />
    <xacro:arg name="prefix" default="gen3_" />

    <joint name="body_gen3" type="fixed">
        <child link="$(arg prefix)base_link" />
        <parent link="body" />
        <origin xyz="0.1656 0 0.12" rpy="0 0 0" />    
    </joint>

    <xacro:include filename="$(find kortex_description)/robots/kortex_robot.xacro" />
    <xacro:load_robot arm="$(arg arm)" gripper="$(arg gripper)" dof="$(arg dof)" vision="$(arg vision)" sim="$(arg sim)" prefix="$(arg prefix)"/>

    <!-- Front CB -->
    <link name="front_collision">
    <visual>
        <geometry>
        <box size="0.2 0.15 0.15" />
        </geometry>
